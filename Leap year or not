#include<stdio.h>
int main()
{
  int year;
  printf("Enter a year: ");
  scanf("%d",&year);

  if(year%400==0) {
    printf("%d is Leap year",year);
  }
  else if(year%100==0) {
    printf("%d is a Leap year",year);
  }
  else if(year%4==0) {
    printf("%d is Leap year",year);
  }
  else {
    printf("%d is Non Leap year",year);
  }
  return 0;
}

/* 
case 1: 
Enter a year: 4
4 is a leap year

Case 2:
Enter a year: 2021
2021 is Non Leap year

Case 3:
Enter a year: 2020
2020 is Leap year

Case 4:
Enter a year: 2023
2023 is Non Leap year
*/
